<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adrw.wallappwallpaper">

    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.adrw.wallappwallpaper.MainActivity2"
            android:label="@string/title_activity_main2">
            <!--android:theme="@style/AppTheme.NoActionBar"-->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.adrw.wallappwallpaper.AboutActivity"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.adrw.wallappwallpaper.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.adrw.wallappwallpaper.MainActivity" />
        <activity android:name="com.adrw.wallappwallpaper.MainActivity">
           <!-- <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name="com.adrw.wallappwallpaper.WallPaperActivity"
            android:theme="@style/AppThemeWallpaper" />

        <receiver android:name="com.adrw.wallappwallpaper.WallpaperAutoChange" />
    </application>

</manifest>